{
  "name": "@nate-stone/piano-keys",
  "version": "0.1.0",
  "description": "Curved piano keyboard React component that renders keys along any SVG path.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./path-presets": {
      "types": "./dist/pathPresets.d.ts",
      "import": "./dist/pathPresets.js",
      "require": "./dist/pathPresets.cjs",
      "default": "./dist/pathPresets.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}
